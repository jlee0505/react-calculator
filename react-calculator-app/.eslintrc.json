{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "react-app", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "max-depth": 2,
    "max-lines": ["error", 15],
    "no-var": "error"
  }
}
